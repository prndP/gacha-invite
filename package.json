    "eslint": "^3.17.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.10.0",
    "exports-loader": "~0.6.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.11.1",
    "filesize": "^3.5.6",
